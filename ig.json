      "template-format": "instance-template-example.html"
    },
    "Observation": {
      "template-format": "instance-template-example.html"
    },
    "Organization": {
      "template-format": "instance-template-example.html"
    },
    "Patient": {
      "template-format": "instance-template-example.html"
    },
    "Procedure": {
      "template-format": "instance-template-example.html"
    },
